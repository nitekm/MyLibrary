<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AllBooksActivity">

	<androidx.recyclerview.widget.RecyclerView
			android:id="@+id/booksRecView"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			app:layout_editor_absoluteX="1dp"
			app:layout_editor_absoluteY="1dp"
			android:layout_margin="15dp" app:layout_constraintEnd_toEndOf="parent"
			tools:layout_editor_absoluteY="15dp" app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>